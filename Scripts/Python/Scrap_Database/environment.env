# Oracle Database Connection Configuration
# Preencha as variáveis abaixo com as informações de conexão

# Host do banco de dados Oracle
ORACLE_HOST=localhost

# Porta do banco de dados (padrão: 1521)
ORACLE_PORT=1521

# Nome do serviço (use ORACLE_SERVICE_NAME ou ORACLE_SID, não ambos)
ORACLE_SERVICE_NAME=ORCL
# ORACLE_SID=ORCL

# Usuário do banco de dados
ORACLE_USER=your_username

# Senha do banco de dados
ORACLE_PASSWORD=your_password

# Schema a ser extraído (deve ser o mesmo do ORACLE_USER ou um schema ao qual o usuário tem acesso)
ORACLE_SCHEMA=your_schema

